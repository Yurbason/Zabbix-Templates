                </macro>
                <macro>
                    <macro>{$AURAVALOPER}</macro>
                    <value>auravaloper1f9xys4xxxxxvte7gljgl4rymmaps7pj3vv4aey</value>
                    <description>auravaloper1xxxxxxxx address of your validator</description>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
