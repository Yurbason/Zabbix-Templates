<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2025-09-12T11:22:40Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>FirmaChain Validators API</template>
            <name>FirmaChain Validators API</name>
            <description>template for FirmaChain network&#13;
done by Yurbason</description>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>CryptoSphere</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>FirmaChain - Delegator APR</name>
                    <type>CALCULATED</type>
                    <key>firma.calculated.delegator.apr</key>
                    <delay>6h</delay>
                    <history>7d</history>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <params>change(&quot;system.run[\&quot;firmachaind query distribution rewards {$FIRMADELEGATORWALLET} -o json --node tcp://localhost:{$FIRMARPCPORT} | jq -r '.total[]' | grep {$FIRMADENOM}\&quot;]&quot;)&#13;
*4&#13;
*100&#13;
*365&#13;
/last(&quot;system.run[\&quot;firmachaind q staking delegations {$FIRMADELEGATORWALLET} -o json --node tcp://localhost:{$FIRMARPCPORT} | jq -r '[.delegation_responses[].balance.amount | tonumber] | add'\&quot;]&quot;)</params>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MATCHES_REGEX</type>
                            <params>^([0-9]*[,.]?[0-9]+|[0-9]+[,.]?[0-9]*)?$</params>
                            <error_handler>DISCARD_VALUE</error_handler>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>FirmaChain - Catching_Up Info</name>
                    <key>system.run[&quot;curl -s http://127.0.0.1:{$FIRMARPCPORT}/status | jq -r '.result.sync_info.catching_up'&quot;]</key>
                    <delay>15m</delay>
                    <history>2d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MATCHES_REGEX</type>
                            <params>^(false|true)$</params>
                            <error_handler>DISCARD_VALUE</error_handler>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{last()}=&quot;true&quot;</expression>
                            <name>FirmaChain - Catching_Up is On</name>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>FirmaChain - Latest Block Height</name>
                    <key>system.run[&quot;curl -s http://127.0.0.1:{$FIRMARPCPORT}/status | jq -r '.result.sync_info.latest_block_height'&quot;]</key>
                    <delay>15m</delay>
                    <history>3d</history>
                    <trends>30d</trends>
                    <units>!</units>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{nodata(30m)}=1</expression>
                            <name>FirmaChain - Latest Block Height no information in 30m</name>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                        <trigger>
                            <expression>{diff()}=0</expression>
                            <name>FirmaChain - Latest Block Height not changed in 15m</name>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>FirmaChain - Quantity of active Proposals</name>
                    <key>system.run[&quot;curl -s http://localhost:{$FIRMAAPIPORT}/cosmos/gov/v1/proposals?proposal_status=PROPOSAL_STATUS_VOTING_PERIOD | jq -r .pagination.total&quot;]</key>
                    <delay>4h</delay>
                    <history>7d</history>
                    <trends>30d</trends>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>NOT_MATCHES_REGEX</type>
                            <params>(.*no proposals.*)</params>
                            <error_handler>CUSTOM_VALUE</error_handler>
                            <error_handler_params>0</error_handler_params>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{diff()}=1</expression>
                            <name>FirmaChain - Quantity of Proposals was changed</name>
                            <priority>INFO</priority>
                            <type>MULTIPLE</type>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>FirmaChain - Missed Blocks Counter</name>
                    <key>system.run[&quot;curl -s http://localhost:{$FIRMAAPIPORT}/cosmos/slashing/v1beta1/signing_infos/{$FIRMAVALCONS} | jq -r .val_signing_info.missed_blocks_counter&quot;]</key>
                    <delay>15m</delay>
                    <history>7d</history>
                    <trends>30d</trends>
                    <units>!blocks</units>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MATCHES_REGEX</type>
                            <params>^[0-9]+$</params>
                            <error_handler>DISCARD_VALUE</error_handler>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{last()} &gt; {last(#2)} + 5</expression>
                            <name>FirmaChain - Missed Blocks Counter was increased</name>
                            <priority>WARNING</priority>
                            <type>MULTIPLE</type>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>FirmaChain - Tombstoned Status</name>
                    <key>system.run[&quot;curl -s http://localhost:{$FIRMAAPIPORT}/cosmos/slashing/v1beta1/signing_infos/{$FIRMAVALCONS} | jq -r .val_signing_info.tombstoned&quot;]</key>
                    <delay>15m</delay>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MATCHES_REGEX</type>
                            <params>^(false|true)$</params>
                            <error_handler>DISCARD_VALUE</error_handler>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{last()}=&quot;true&quot;</expression>
                            <name>FirmaChain - Tombstoned is True</name>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>FirmaChain - Jailed Status</name>
                    <key>system.run[&quot;curl -s http://localhost:{$FIRMAAPIPORT}/cosmos/staking/v1beta1/validators/{$FIRMAVALOPER} | jq -r .validator.jailed&quot;]</key>
                    <delay>15m</delay>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MATCHES_REGEX</type>
                            <params>^(false|true)$</params>
                            <error_handler>DISCARD_VALUE</error_handler>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{last()}&lt;&gt;&quot;false&quot;</expression>
                            <name>FirmaChain - Validator was Jailed</name>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>FirmaChain - Bond Status</name>
                    <key>system.run[&quot;curl -s http://localhost:{$FIRMAAPIPORT}/cosmos/staking/v1beta1/validators/{$FIRMAVALOPER} | jq -r .validator.status&quot;]</key>
                    <delay>15m</delay>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MATCHES_REGEX</type>
                            <params>^.*BOND.*$</params>
                            <error_handler>DISCARD_VALUE</error_handler>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{last()}=&quot;BOND_STATUS_UNBONDING&quot;</expression>
                            <name>FirmaChain - Bond Status is UNBONDED!</name>
                            <priority>HIGH</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                        <trigger>
                            <expression>{diff()}=1</expression>
                            <name>FirmaChain - Bond Status was changed</name>
                            <priority>INFO</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>FirmaChain - Minimum signed per window</name>
                    <key>system.run[&quot;firmachaind q slashing params -o json --node tcp://localhost:{$FIRMARPCPORT} | jq -r .params.min_signed_per_window&quot;]</key>
                    <delay>12h</delay>
                    <history>7d</history>
                    <trends>90d</trends>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>100</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>FirmaChain - Signed Blocks Window</name>
                    <key>system.run[&quot;firmachaind q slashing params -o json --node tcp://localhost:{$FIRMARPCPORT} | jq -r .params.signed_blocks_window&quot;]</key>
                    <delay>12h</delay>
                    <history>7d</history>
                    <trends>90d</trends>
                    <units>!blocks</units>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>FirmaChain - Slash Downtime</name>
                    <key>system.run[&quot;firmachaind q slashing params -o json --node tcp://localhost:{$FIRMARPCPORT} | jq -r .params.slash_fraction_downtime&quot;]</key>
                    <delay>12h</delay>
                    <history>7d</history>
                    <trends>90d</trends>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>100</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>FirmaChain - Delegator Total Delegations</name>
                    <key>system.run[&quot;firmachaind q staking delegations {$FIRMADELEGATORWALLET} -o json --node tcp://localhost:{$FIRMARPCPORT} | jq -r '[.delegation_responses[].balance.amount | tonumber] | add'&quot;]</key>
                    <delay>1d</delay>
                    <history>7d</history>
                    <trends>30d</trends>
                    <value_type>FLOAT</value_type>
                    <units>!FCT</units>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>0.000001</params>
                        </step>
                        <step>
                            <type>JAVASCRIPT</type>
                            <params>return Math.round(value* 1000) / 1000</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>FirmaChain - Delegations Unbonding Time</name>
                    <key>system.run[&quot;firmachaind q staking params -o json --node tcp://localhost:{$FIRMARPCPORT} | jq -r .params.unbonding_time&quot;]</key>
                    <delay>1d</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>FirmaChain - Validator Commission Rewards</name>
                    <key>system.run[&quot;firmachaind query distribution commission {$FIRMAVALOPER} -o json --node tcp://localhost:{$FIRMARPCPORT} | jq -r '.commission.commission[]' | grep {$FIRMADENOM}&quot;]</key>
                    <delay>6h</delay>
                    <history>7d</history>
                    <trends>90d</trends>
                    <value_type>FLOAT</value_type>
                    <units>!FCT</units>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>STR_REPLACE</type>
                            <params>ufct
</params>
                        </step>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>0.000001</params>
                        </step>
                        <step>
                            <type>JAVASCRIPT</type>
                            <params>return Math.round(value* 1000) / 1000</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>FirmaChain - Delegator Rewards</name>
                    <key>system.run[&quot;firmachaind query distribution rewards {$FIRMADELEGATORWALLET} -o json --node tcp://localhost:{$FIRMARPCPORT} | jq -r '.total[]' | grep {$FIRMADENOM}&quot;]</key>
                    <delay>6h</delay>
                    <history>7d</history>
                    <trends>90d</trends>
                    <value_type>FLOAT</value_type>
                    <units>!FCT</units>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>STR_REPLACE</type>
                            <params>ufct
</params>
                        </step>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>0.000001</params>
                        </step>
                        <step>
                            <type>JAVASCRIPT</type>
                            <params>return Math.round(value* 1000) / 1000</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>FirmaChain - Validator Voting Power</name>
                    <key>system.run[&quot;firmachaind query staking validator {$FIRMAVALOPER} -o json --node tcp://localhost:{$FIRMARPCPORT} | jq -r '.validator.tokens'&quot;]</key>
                    <delay>1h</delay>
                    <history>7d</history>
                    <trends>90d</trends>
                    <value_type>FLOAT</value_type>
                    <units>!FCT</units>
                    <applications>
                        <application>
                            <name>CryptoSphere</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>0.000001</params>
                        </step>
                        <step>
                            <type>JAVASCRIPT</type>
                            <params>return Math.round(value* 1000) / 1000</params>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{diff()}=1</expression>
                            <name>FirmaChain - Validator Voting Power was changed</name>
                            <priority>INFO</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
            </items>
